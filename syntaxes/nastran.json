{
    "name": "nastran",
	"scopeName": "source.nastran",
	"fileTypes": ["bdf", "dat", "nas", "inp"],
	"patterns": [
	{
		"begin": "\"",
		"name": "string.quoted.double.untitled",
		"patterns": [
		{
			"name": "constant.character.escape.nastran",
			"match": "//."
		}],
		"end": "\""
	},
	{
		"captures":
		{
			"1":
			{
				"name": "comment.line.character.nastran"
			}
		},
		"name": "comment.line.character.nastran",
		"match": "(\\$).*$\\n?"
	},
	{
        "include": "#nastran"
    },
	{
        "include": "#section"
    },
	{
		"include": "#exec"
	},
	{
		"include": "#bulk"
	},
	{
		"include": "#case"
	},
	{
		"include": "#plot"
	},
	{
		"name": "string.quoted.single.nastran",
		"beginCaptures":
		{
			"0":
			{
				"name": "punctuation.definition.string.begin.nastran"
			}
		},
		"end": "'",
		"comment": "single quoted string (does not allow interpolation)",
		"endCaptures":
		{
			"0":
			{
				"name": "punctuation.definition.string.end.nastran"
			}
		},
		"begin": "'",
		"patterns": [
		{
			"name": "constant.character.escape.nastran",
			"match": "\\\\'|\\\\\\\\"
		}]
	},
	{
		"name": "string.quoted.double.nastran",
		"beginCaptures":
		{
			"0":
			{
				"name": "punctuation.definition.string.begin.nastran"
			}
		},
		"end": "'",
		"comment": "single quoted string (does not allow interpolation)",
		"endCaptures":
		{
			"0":
			{
				"name": "punctuation.definition.string.end.nastran"
			}
		},
		"begin": "'",
		"patterns": [
		{
			"name": "constant.character.escape.nastran",
			"match": "\\\\\"|\\\\\\\\"
		}]
	}],
	"repository":
	{
		"nastran":
        {
            "captures":
            {
                "1":
                {
                    "name": "string.regexp.nastran"
                }
            },
            "match": "^\\s*(?i)(NASTRAN)"
        },
		"section":
        {
            "captures":
            {
                "1":
                {
                    "name": "invalid.section.nastran"
                }
            },
            "match": "^\\s*(?i)(ID|ALTER|CEND|BEGIN BULK|SUBSTRUCTURE|ENDDATA|ENDALTER|ENDSUBS|END)"
        },
		"exec":
        {
            "captures":
            {
                "1":
                {
                    "name": "string.exec.nastran"
                }
            },
            "match": "^\\s*(?i)(ALTER|APP|BEGIN|CEND|CHKPNT|DELETE|DIAG|ENDALTER|END|ID|INSERT|NUMF|READFILE|RESTART|SOL|TIME|UMFEDIT|UMF)"
        },
		"subs":
        {
            "captures":
            {
                "1":
                {
                    "name": "markup.inserted.subs.nastran"
                }
            },
            "match": "^\\s*(?i)(BRECOVER|CHECK|COMBINE|CREDUCE|DELETE|DESTROY|DUMP|EDIT|ENDSUBS|EQUIV|MRECOVER|MREDUCE|OPTIONS|OPTION|PASSWORD|PLOT|RECOVER|REDUCE|RESTORE|RUN|SOFIN|SOFOUT|SOFPRINT|SOFP|SOF|SOLVE|SUBSTRUCTURE)"
        },
